                    <form action="/home">
                        <h2>Login</h2><br><br><br>
                        <div class="form-group">
                          <label for="exampleInputEmail1">Correo electrónico</label>
                          <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
